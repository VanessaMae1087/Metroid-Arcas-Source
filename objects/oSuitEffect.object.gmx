            <string>/// Effect Shape

switch (Step)
{
 case -1: // Intro sounds
  {
   Wait --;
   if Wait &lt; 0
   {
    Width = 1;
    Step = 0;
    sound_play_pos(sndSuit,x,y);
   }
  }
 break;
 
 case 0:  // Open vertically
  {
   Height += 8;
   if Height &gt; 240
    Step = 1;
  }
 break;
 
 case 1:  // Open horizontally
  {
   Width += 10;
   if Width &gt; 240
   {
    Step = 2;  
    global.LastSuit = global.Suit; 
   }
  }
 break; 
 
 case 2:  // Close vertically
  {
   Height -= 6;
   if Height &lt; 1
   {
    Step = 3;
    Height = 1;
   }
  }
 break;

 case 3:  // Close horizontally
  {
   Width -= 8;
   if Width &lt; 4
    instance_destroy();
  }
}
</string>
